<body>
    <% if(locals.isAuthenticated){ %>
        <div class="navbar navbar-default navbar-static-top" role="navigation">
            <div class="container-fluid container-wide">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
                    <a class="navbar-brand" href="/dashboard">SeoDr</a>
                </div>

                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li class="js_page_/dashboard js_page_summary" <% if (locals.path=='/dashboard' ){%>class="active"
                            <% } %>><span class="js_count"></span><a href="/dashboard">Home</a></li>
                        <li class="js_page_/profile profile" <% if (locals.path=='/profile' ){%>class="active"
                            <% } %>><span class="js_count"></span><a href="/profile">Profile</a></li>
                        <li class="js_page_/billing billing" <% if (locals.path=='/billing' ){%>class="active"
                            <% } %>><span class="js_count"></span><a href="/billing">Billing</a></li>
                        <li class="js_page_/scan scan" <% if (locals.path=='/scan' ){%>class="active"
                            <% } %>><span class="js_count"></span><a href="/scan">Scan</a></li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right">
                        <li <% if (locals.path !=='/' ){%>class="active"
                            <% } %>><a href="/logout">Sign Out</a></li>
                        <li <% if (locals.path !=='/' ){%>class="active"
                            <% } %>><a href="/users">Users</a></li>
                        <li <% if (locals.path=='/' ){%>class="active"
                            <% } %>><a href="/">Log in</a></li>
                        <li <% if (locals.path=='/' ){%>class="active"
                            <% } %>><a href="/signup">Sign Up</a></li>
                        <li <% if (locals.path=='/' ){%>class="active"
                            <% } %>><a href="/forgot">Forgot Password</a></li>
                        <li <% if (locals.path=='/activity' ){%>class="active"
                            <% } %>><a href="/activity">Activity</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <% } %>
            <%- body %>
                <% include messages/flash %>

                    <!-- User Information -->
                    <script>
                        <% if(locals.user && locals.user.uid){ %>
                        window.uid = '<%= user.uid %>';
                        window.apiToken = '<%= user.apiToken %>';
                        <% } %>
                        window.CURRENT_HOST = '<%= CURRENT_HOST %>';
                    </script>

                    <!-- Concat Depends -->
                    <script src="/javascripts/vendor/jquery.min.js"></script>
                    <script src="/javascripts/vendor/react.min.js"></script>
                    <script src="/javascripts/vendor/react-do.js"></script>
                    <script src="/javascripts/vendor/react-infinite.min.js" type="text/jsx"></script>
                    <script src="/javascripts/vendor/precompiledinsteadofthis____jsTransformer.js"></script>
                    <script src="/javascripts/vendor/d3.min.js"></script>
                    <script src="/javascripts/vendor/d3-timeline.min.js"></script>
                    <script src="/javascripts/vendor/moment.min.js"></script>
                    <script src="/javascripts/vendor/underscore.min.js"></script>
                    <script src="/javascripts/vendor/bootstrap.min.js"></script>
                    <script src="/javascripts/vendor/TweenMax.min.js"></script>
                    <script src="/javascripts/vendor/ejs.js"></script>

                    <!-- Local Scripts -->
                    <script src="/javascripts/app/app.js"></script>
                    <script src="/javascripts/app/callbacks.js"></script>



                    <!-- Only needed for signup and changing timezone -->
                    <script src="/javascripts/vendor/moment-timezone-with-data.js"></script>
                    <!-- Profile Page Only -->
                    <script src="/javascripts/vendor/bootstrap-select.min.js"></script>
                    <link rel="stylesheet" href="/stylesheets/bootstrap-select.min.css">

                    <script>
                        <% if(locals && locals.user){ %>
                        $(function($) {
                            <% if (locals.path=='/' ){%>
                            var timezone = moment.tz.guess();
                            $('.timezone-select').val(timezone);
                            <% } %>
                            var timezone = moment.tz.names();
                            for (var i = 0; i < timezone.length; i++) {
                                $('.timezone-select').append('<option value="' + timezone[i] + '">' + timezone[i] + '</option>');
                            }
                            $('.timezone-select').selectpicker('val', "<%=user.timezone%>");
                        });
                        <% } %>
                    </script>

                    <!-- Stripe -->
                    <script type="text/javascript" src="https://js.stripe.com/v2/"></script>
                    <% if(locals.stripePubKey){ %>
                        <script type="text/javascript">
                            Stripe.setPublishableKey('<%= stripePubKey %>');
                        </script>
                        <% } %>

                            <!-- Socket.io -->
                            <script src="<%= CURRENT_HOST %>/socket.io/socket.io.js"></script>

</body>

</html>
