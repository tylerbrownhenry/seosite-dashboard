            </div>

            <h3>Scans</h3>
            < %=s cans.list.length %>
                <div class="container-fluid container-max">
                    <table class="scans table">
                        < % for(var i=0; i<scans.list.length; i++) {%>
                            < % if(scans.list[i].url && scans.list[i].url.url){%>
                                <tr class="< %= scans.list[i].requestId %>">
                                    <td>Scan</td>
                                    <td>Url:
                                        < %=s cans.list[i].url.url %>
                                    </td>
                                    <td>Completed :
                                        < %=s cans.list[i].completedTime %>
                                    </td>
                                    <td><button>summary</button><button>resources</button><button>meta</button><button>links</button><button>issues</button><button>security</button></td>
                                </tr>
                                < % } else { %>
                                    <tr>
                                        <td>Request</td>
                                        <td>Url:
                                            < %=scans.list[i].url %>
                                        </td>
                                        <td>Failed:
                                            < %=scans.list[i].failedReason %> Request Time:
                                                < %=scans.list[i].requestDate %>
                                        </td>
                                        <td></td>
                                    </tr>
                                    < % } %>
