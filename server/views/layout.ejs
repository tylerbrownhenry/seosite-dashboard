<!doctype html>
<html class="no-js app-temp">
  <head>
    <meta charset="utf-8">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" href="/stylesheets/style.css">

    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,400,400i,700,700i|Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap-theme.min.css">


  </head>


    <div class="alert alert-warning alert-dismissible disconnected alert" role="alert" style="margin:0;display:none">
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
      <strong>No Biggie.</strong> We lost your connection, but we'll find it!
    </div>

    <div class="alert alert-info alert-dismissible" id="info" role="alert" style="margin:0;display:none">
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
      <strong>Whew whew!</strong> Your request is complete!
    </div>


  <body>
    <div class="navbar navbar-default navbar-static-top" role="navigation">
      <div class="container-fluid container-wide">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/dashboard">SeoDr</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li <% if (locals.path == '/dashboard'){%>class="active"<% } %>><a href="/dashboard">Home</a></li>
            <li <% if (locals.path == '/profile'){%>class="active"<% } %>><a href="/profile">Profile</a></li>
            <li <% if (locals.path == '/billing'){%>class="active"<% } %>><a href="/billing">Billing</a></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li <% if (locals.path !== '/'){%>class="active"<% } %>><a href="/logout">Sign Out</a></li>
            <li <% if (locals.path == '/'){%>class="active"<% } %>><a href="/">Log in</a></li>
            <li <% if (locals.path == '/'){%>class="active"<% } %>><a href="/signup">Sign Up</a></li>
            <li <% if (locals.path == '/'){%>class="active"<% } %>><a href="/forgot">Forgot Password</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>
    <%- body %>
    <% include dashboard/flash %>
    <!-- Google Analytics: change UA-XXXXX-X to be your site's ID -->
    <script>
     (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
     (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
     m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
     })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

     ga('create', '<----%= googleAnalytics %>');
     ga('send', 'pageview');
    </script>


     <script>
        <% if(locals.user && locals.user.uid){ %>
         window.uid = '<%= user.uid %>';
        <% } %>
        window.CURRENT_HOST = '<%= CURRENT_HOST %>';
    </script>

    <script src="/javascripts/jquery.min.js"></script>
    <script src="/javascripts/d3.min.js"></script>
    <script src="/javascripts/d3-timeline.min.js"></script>
    <script src="/javascripts/moment.min.js"></script>
    <script src="/javascripts/underscore.min.js"></script>
    <script src="/javascripts/app.js"></script>

    <script type="text/javascript" src="https://js.stripe.com/v2/"></script>
        <% if(locals.stripePubKey){ %>
    <script type="text/javascript">
      Stripe.setPublishableKey('<%= stripePubKey %>');
   
    </script>
        <% } %>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.19.1/TweenMax.min.js"></script>


    <script src="<%= CURRENT_HOST %>/socket.io/socket.io.js"></script>
